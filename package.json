    "bittorrent-swarm": "^5.1.2",
    "convict": "^1.0.1",
    "create-torrent": "^3.15.1",
    "debug": "^2.2.0",
    "hat": "0.0.3",
    "magnet-uri": "^5.0.0",
    "nconf": "^0.8.2",
    "parallel": "^0.2.1",
    "parse-torrent": "^5.4.0",
    "random-iterate": "^1.0.1",
    "re-emitter": "^1.1.1",
